{
  "name": "gaia",
  "version": "0.1.0",
  "private": true,
  "description": "Gaia astrological charting application",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:unit": "pnpm --filter './packages/*' test",
    "test:backend": "cd ../coeus-api && pytest",
    "test:integration": "cd ../coeus-api && pytest tests/integration -m integration",
    "test:coverage": "pnpm -r test --coverage"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}

